#include <HUBeeBMDWheel.h> 


int pin1=13;
int pin2=12;
int pwm1=11;
int pin3=10;
int pin4=8;
int pwm2=9;


HUBeeBMDWheel leftWheel(13 , 12, 11);
HUBeeBMDWheel rightWheel(10,8,9);

void setup(){

pinMode(7,OUTPUT);
  digitalWrite(7,HIGH);
  
}

void loop (){
  
  rightWheel.setMotorPower(-255);
  
  leftWheel.setMotorPower(255);
  
}
